package com.harsha.interpe2;

public class Task4 {

	public static void main(String[] args) {
		String s1 = "* * * * * * ================================== \n * * * * *  ==================================";
		
		for(int i=0;i<4;i++){
			System.out.println(s1);
		}
		System.out.println("* * * * * * ==================================");
		for(int i = 0;i<5;i++){
			System.out.println("==============================================");
		}
	}

}
